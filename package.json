{
  "name": "ts-node-react-vercel-deploy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && npx tsc && babel dist -d dist",
    "start": "npx tsc && babel dist -d dist && npx kill-port 4000 && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "server:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "dev:prod": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "log-level:change": "runtime-node-refresh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
