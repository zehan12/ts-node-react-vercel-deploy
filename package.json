{
  "name": "ts-node-react-vercel-deploy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx jest --clearCache && jest --coverage",
    "build": "rm -rf dist && npx tsc && babel dist -d dist",
    "start": "npx tsc && babel dist -d dist && npx kill-port 4000 && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "server:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "dev:prod": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "build-client": "mkdir -p build && cd client && npm run build && cp -r ./dist/* ../build/",
    "log-level:change": "runtime-node-refresh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.15",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
