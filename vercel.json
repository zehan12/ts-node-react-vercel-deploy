{
    "version": 2,
    "builds": [
      {
        "src": "/dist/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "/build/index.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "src": "/api",
        "dest": "/dist/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/index.html"
      }
    ],
    "cleanUrls": true
  }
  