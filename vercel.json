{
    "version": 2,
    "builds": [
      {
        "src": "/dist/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api",
        "dest": "/dist/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/index.html"
      }
    ]
  }
  